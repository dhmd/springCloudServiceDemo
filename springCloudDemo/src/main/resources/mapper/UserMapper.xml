<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiuxiniot.example.dto.mapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.jiuxiniot.example.entity.User">
        <id column="id" property="id"/>
        <result column="avatar" property="avatar"/>
        <result column="account" property="account"/>
        <result column="password" property="password"/>
        <result column="salt" property="salt"/>
        <result column="name" property="name"/>
        <result column="birthday" property="birthday"/>
        <result column="sex" property="sex"/>
        <result column="email" property="email"/>
        <result column="phone" property="phone"/>
        <result column="role_id" property="roleId"/>
        <result column="org_id" property="orgId"/>
        <result column="actived" property="actived"/>
        <result column="version" property="version"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="quarters" property="quarters"/>
        <result column="last_login_time" property="lastLoginTime"/>
        <result column="last_login_location" property="lastLoginLocation"/>
        <result column="pay_password" property="payPassword"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, avatar, account, password, salt, name, birthday,quarters, sex, email, phone, role_id, org_id, actived, version, create_time, update_time,
        last_login_time,last_login_location,pay_password
    </sql>


</mapper>
